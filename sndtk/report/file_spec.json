        },
        {
          "testname": "test__FileReport__uncovered_count__returns_zero_for_target_function_when_target_is_covered",
          "description": "Returns zero for target function when target is covered"
        },
        {
          "testname": "test__FileReport__uncovered_count__returns_zero_when_target_function_does_not_exist",
