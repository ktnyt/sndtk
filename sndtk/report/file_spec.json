        },
        {
          "testname": "test__FileReport____str____returns_checkmark_when_covered",
          "description": "Returns checkmark string when all functions are covered"
        },
        {
          "testname": "test__FileReport____str____returns_cross_with_reports_when_uncovered",
          "description": "Returns cross string with function reports when uncovered"
        }
      ]
    },
    {
      "identifier": "FileReport::uncovered_count",
      "scenarios": [
        {
          "testname": "test__FileReport__uncovered_count__returns_zero_when_empty",
          "description": "Returns zero when functions list is empty (boundary value)"
        },
        {
          "testname": "test__FileReport__uncovered_count__returns_zero_when_all_covered",
          "description": "Returns zero when all functions are covered"
        },
        {
          "testname": "test__FileReport__uncovered_count__returns_one_when_single_uncovered",
          "description": "Returns one when single function is uncovered (boundary value)"
        },
        {
          "testname": "test__FileReport__uncovered_count__returns_count_when_some_uncovered",
          "description": "Returns correct count when some functions are uncovered"
        },
        {
          "testname": "test__FileReport__uncovered_count__returns_count_when_all_uncovered",
          "description": "Returns correct count when all functions are uncovered (boundary value)"
        },
        {
          "testname": "test__FileReport__uncovered_count__returns_count_for_target_function_when_target_is_specified",
