        },
        {
          "testname": "test__FileReport__get_first_uncovered_function__returns_first_uncovered_function",
          "description": "Returns first uncovered function correctly"
        },
        {
          "testname": "test__FileReport__get_first_uncovered_function__returns_none_when_all_covered",
          "description": "Returns None when all functions are covered"
        }
      ]
    },
    {
      "identifier": "FileReport::covered",
      "scenarios": [
        {
          "testname": "test__FileReport__covered__returns_true_when_empty",
          "description": "Returns True when functions list is empty (boundary value)"
        },
        {
          "testname": "test__FileReport__covered__returns_true_when_all_covered",
          "description": "Returns True when all functions are covered"
        },
        {
          "testname": "test__FileReport__covered__returns_false_when_some_uncovered",
          "description": "Returns False when some functions are uncovered"
        }
      ]
    },
    {
      "identifier": "FileReport::__str__",
      "scenarios": [
        {
          "testname": "test__FileReport____str____returns_emoji_when_empty",
